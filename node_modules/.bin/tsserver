#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/ysquares/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/tsserver/node_modules:/home/workspace/nodejs/ysquares/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/home/workspace/nodejs/ysquares/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/home/workspace/nodejs/ysquares/node_modules/.pnpm/typescript@3.9.10/node_modules:/home/workspace/nodejs/ysquares/node_modules/.pnpm/node_modules:/home/workspace/nodejs/ysquares/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/ysquares/node_modules/typescript/bin/tsserver/node_modules:/home/workspace/nodejs/ysquares/node_modules/typescript/bin/node_modules:/home/workspace/nodejs/ysquares/node_modules/typescript/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
